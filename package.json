  "prisma": {
    "seed": "ts-node prisma/scripts/seed.ts"
  },
  "scripts": {
    "sync": "npx prisma db pull && npx prisma migrate dev",
    "migrate": "npx prisma migrate dev --name auto",
    "delete": "ts-node prisma/scripts/deleteBackend.ts",
    "reset": "yarn delete && yarn migrate",
    "dev": "ts-node prisma/scripts/init.ts && next dev & npx prisma studio --browser none & yarn storybook",
    "build": "next build",
    "export": "next export",
    "build-mobile": "next build && next export && npx cap sync",
    "start": "next start",
    "lint": "next lint",
    "storybook": "yarn start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test-storybook": "test-storybook"
